    <logoImage>C:\Users\jackp\Downloads\Echo Engine Installer.png</logoImage>
    <componentList>
        <component name="default">
            <description>Default Component</description>
            <canBeEdited>1</canBeEdited>
            <selected>1</selected>
            <show>1</show>

            <!-- Project files -->

            <folderList>
                <folder destination="${installdir}" name="programfileslinux">
                    <description>Program Files</description>
                    <platforms>linux</platforms>
                    <distributionFileList>
                        <distributionDirectory>
                            <origin>C:/Users/jackp/Downloads/Linux/EchoEngine</origin>
                        </distributionDirectory>
                    </distributionFileList>
                </folder>
            </folderList>
        </component>
    </componentList>
    <enableRollback>1</enableRollback>
    <enableTimestamp>1</enableTimestamp>
